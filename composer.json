{
  "name": "mint-software/guzzle-retry-middleware",
  "description": "Mint Software Guzzle retry middleware",
  "keywords": [
    "guzzle",
    "retry",
    "middleware"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Krzysztof SÅ‚omka",
      "email": "kisztof@gmail.com"
    }
  ],
  "require": {
    "php": "~7.1",
    "guzzlehttp/guzzle": "^6.3",
    "symfony/http-foundation": "^4.2"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.0",
    "squizlabs/php_codesniffer": "^3.4"
  },
  "autoload": {
    "psr-4": {
      "MintSoftware\\GuzzleRetry\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "MintSoftware\\GuzzleRetry\\": "tests"
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit --coverage-text",
    "phpcs": "vendor/bin/phpcs -p --standard=PSR2 --runtime-set ignore_errors_on_exit 1 --runtime-set ignore_warnings_on_exit 1 src tests"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "master"
    }
  },
  "config": {
    "sort-packages": true
  }
}
